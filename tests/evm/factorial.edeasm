    PUSH UI64 5         # num: the number the take the factorial of
    PUSH UI64 1         # The result
@LOOP:
    SLOAD -16       # push num
    PUSH UI64 1     # push 1 to compare num to
    EQ UI64         # push num == 1
    JUMPNZ @EXIT
    SLOAD -16       # push num
    MUL UI64        # result *= num 
    PUSH UI64 1
    SLOAD -24       # push num
    SUB UI64
    SSTORE -16      # num -= 1
    JUMP @LOOP
@EXIT:
    CONVERT UI64 I64
    EXIT